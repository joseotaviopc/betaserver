{
  "name": "betaserver",
  "version": "0.0.1",
  "description": "Beta app server",
  "author": "Jos√© Otavio",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "tsc -p tsconfig.build.json && cp -R ./register-path.js tsconfig.build.json package.json ./dist",
    "dev": "PROFILEHOME=development tsnd -r dotenv/config -r tsconfig-paths/register ./src/main.ts",
    "prod": "PROFILEHOME=development node -r dotenv/config -r ./dist/register-path.js ./dist/src/main.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "keywords": [],
  "devDependencies": {
    "@prisma/client": "^4.16.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@expressots/core": "latest",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-express-utils": "^6.3.2",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^4.1.2"
  }
}